# - Config file for the LeapHTTP package
# It defines the following variables
#  LeapHTTP_FOUND - indicates that the module was found
#  LeapHTTP_INCLUDE_DIR - include directories

# Check if local build
if ("@CMAKE_CURRENT_BINARY_DIR@" STREQUAL CMAKE_CURRENT_LIST_DIR)
  set(LeapHTTP_INCLUDE_DIR "@PROJECT_SOURCE_DIR@")
  include("${CMAKE_CURRENT_LIST_DIR}/LeapHTTPTargets.cmake")
else()
  set(LeapHTTP_INCLUDE_DIR "${CMAKE_CURRENT_LIST_DIR}/include")
  include("${CMAKE_CURRENT_LIST_DIR}/cmake/LeapHTTPTargets.cmake")
endif()

set(LeapHTTP_FOUND TRUE)
